{
  "expo": {
    "name": "QFlow",
    "slug": "QFlow",
    "scheme": "QFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#a976e5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#a976e5"
      }
    },
    "web": {
      "favicon": "./assets/mages/icon.png"
    },
    "plugins": ["expo-router"]
  }
}
